Vagrant.configure("2") do |config|
	config.vm.define "homework-ansible" do |h|
		h.vm.hostname = "homework-ansible.localdomain"
			h.vm.box = "ubuntu/jammy64"
			h.vm.box_check_update = false
            h.vm.network "private_network", ip: "192.168.56.6"
			h.vm.provider "virtualbox" do |vb|
			        vb.memory = "1024"
			        vb.cpus = "2"
			end
	  end
end
